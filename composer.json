{
    "name": "gemorroj/htmlvalidator",
    "type": "library",
    "description": "W3C HTML Validator service.",
    "license": "LGPL-3.0-or-later",
    "require": {
        "php": ">=7.4",
        "symfony/http-client": "^4.4.11||^5.0||^6.0"
    },
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5.16",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "HTMLValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HTMLValidator\\Tests\\": "tests/"
        }
    }
}
